{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "public/index.html"
      },
      {
        "src": "/ocr",
        "dest": "api/app.py"
      }
    ]
  }