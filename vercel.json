{
  "version": 2,
  "builds": [
    {
      "src": "Dockerfile",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "api"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "api/app.py"
    },
    {
      "src": "/ocr",
      "dest": "api/app.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "api/static/$1"
    }
  ]
}