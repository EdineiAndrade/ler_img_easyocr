{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "api/app.py"
    },
    {
      "src": "/ocr",
      "dest": "api/app.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "api/static/$1"
    }
  ]
}